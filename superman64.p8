function make_map_block(kind,x,y,lvl)
	local b = {}
	b.kind = kind
	b.h = 1
	b.w = 1
	b.x = x
	b.y = y
	if(lvl == 1) then
		add(level1,b)
	elseif(lvl == 2) then
		add(level2,b)
	elseif(lvl == 3) then
		add(level3,b)
	end
	return b
end

function make_actor(kind,x,y,d,w,h)
	local a = {}
	a.kind = kind
	a.life = 1
	a.x=x a.y=y a.dx=0 a.dy=0
--	a.ddy=0.06 --gravity
	a.w=w a.h=h
	a.d=d
 add(actors,a)
 return a
end

function make_player(x,y,d)
	local p1 = make_actor(0,x,y,d,2,1)
	p1.score = 0
	p1.id = 0 --player 1
	p1.frame = 1
	return p1
end

function make_ring(x,y,d)
	local r = make_actor(1,x,y,d,1,1)
	r.frame = 25
	r.i = 0
	return r
end

function move_player(p1)
	if(p1.life == 0) then --death
		del(actors,p1)
		music(-1)
		return
	end
	
	accel = .05
	
	--player controls
	if(btn(0))then
		p1.dx = p1.dx - accel; p1.d=0 end
	if(btn(1))then
		p1.dx = p1.dx + accel; p1.d=1 end
	if(btn(2))then
		p1.dy = p1.dy - accel; p1.d=2 end
	if(btn(3))then
		p1.dy = p1.dy + accel; p1.d=3 end	
			
end

function update_ring(r)
	r.i += 1
	if r.i == 41 then
		r.i = 0 
	end
	if r.i <=10 then 
		r.frame = 24
	elseif r.i >10 and r.i <= 20 then r.frame = 40
	elseif r.i > 20 and r.i <= 30 then r.frame = 56
	elseif r.i > 30 and r.i <= 40 then r.frame = 25 end
end

