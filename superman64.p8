function _init()
	cls()
	actors={}
	level1={}
	level2={}
	level3={}
	level = 1
	rings = 0
	run_level1()
end

function run_level1()
	player = make_player(64,64,1)
	for i=0,5 do
		for j=0,16 do
			make_map_block(33,i*8,j*8,1)
		end
	end
	for i=5,13 do
		for j=0,16 do
			make_map_block(17,i*8,j*8,1)
		end
	end
	for i = 13,16 do
		for j=0,16 do
			make_map_block(33,i*8,j*8,1)
		end
	end
	for i=1,5 do
		make_ring(rnd(100),rnd(100),1)
	end
end

