function _init()
	cls()
	--preset values
	actors={}
	level1={}
	level2={}
	level3={}
	--current level status and values
	currentlvl ={}
	level = 0
	x_dist = 0
	prev_r = {}
	score = 0
	--inits
	rings = 0
    enemies = 0
	timer = 6
	max_rings = 5
    max_enemies = 5
	once = false
	active = false
	next = false
	power_dir = 1
	make_levels()	
	run_level1()
	music(0) 
end

