function run_level1()
	level = 1
 timer = 26
	actors={}
	currentlvl = level1
	player = make_player(64,64,1)
	make_obstacle(136,64,1)
	make_ring(136,rnd(100),1)
end

function run_level2()
 rings = 0
	timer = 6
	level = 2
	actors = {}
	currentlvl = level2
	player = make_player(20,90,1)
	hoop = make_hoop (88,32,1)
	car = make_car(20,64,1)
end

function make_map_block(kind,x,y,lvl)
	local b = {}
	b.kind = kind
	b.h = 1
	b.w = 1
	b.x = x
	b.y = y
	if(lvl == 1) then
		add(level1,b)
	elseif(lvl == 2) then
		add(level2,b)
	elseif(lvl == 3) then
		add(level3,b)
	end
	return b
end

function make_actor(kind,x,y,d,w,h)
	local a = {}
	a.kind = kind
	a.life = 1
	a.x=x a.y=y a.dx=0 a.dy=0
	a.ddy=0.00 --gravity
	a.w=w a.h=h
	a.d=d
 add(actors,a)
 return a
end

function make_player(x,y,d)
	local p1 = {}
	if(level==1)then 
		p1 = make_actor(0,x,y,d,2,1)
		p1.frame = 1
	elseif(level==2)then 
		p1 = make_actor(0,x,y,d,1,2)
		p1.frame=10 
	end
	p1.score = 0
	p1.id = 0 --player 1
	return p1
end

function make_car(x,y,d)
	local c = make_actor(3,x,y,d,4,2)
	c.frame = 11
	c.power = 0
	return c
end

function make_hoop(x,y,d)
	local c = make_actor(5,x,y,d,4,2)
	c.frame = 64
	return c
end

function make_obstacle(x,y,d)
	local o = make_actor(2,x,y,d,1,1)
	o.frame = 43
	return o
end

function make_enemy(x,y,d)
    enemies += 1
    local e = make_actor(4,x,y,d,1,1)
    e.frame = 44
    return e
end

function make_ring(x,y,d)
	rings = rings + 1
	if(y > 110) then x = 120 end
	if(y < 10) then x = 5 end
	local r = make_actor(1,x,y,d,1,1)
	r.frame = 25
	r.i = 0
	prev_r = r
	return r
end

function move_player(p1)
	if(p1.life == 0) then --death
		del(actors,p1)
		music(-1)
		return
	end
	
	accel = .05
	
	--player controls
	if(level==1)then
		if(btn(0))then
			p1.dx = p1.dx - accel; p1.d=0 end
		if(btn(1))then
			p1.dx = p1.dx + accel; p1.d=1 end
		if(btn(2))then
			p1.dy = p1.dy - accel end
		if(btn(3))then
			p1.dy = p1.dy + accel end 
	elseif(level==2)then
		p1.d=2
		if(btnp(5))then
			once = true;
			active = true;
		end
	end
	x_dist += 1.5
end


function collide(a1, a2)
 if (a1==a2) then return end
 local a1_cent = {a1.x+(a1.w/2),a1.y+(a1.h/2)}
 local a2_cent = {a2.x+(a2.w/2),a2.y+(a2.h/2)}
 local dist = sqrt((a2_cent[1] - a1_cent[1])*
                   (a2_cent[1] - a1_cent[1]) + 
                   (a2_cent[2] - a1_cent[2])*
                   (a2_cent[2] - a1_cent[2]))
 if(a2.kind == 4)then 
    printh("dist: "..dist.."/ x: "..a2.x)  end
 if dist < (a1.w + a2.w)*2 and dist >= 0 then
	collide_event(a1, a2)
 end	
end


function collisions()
 for a in all(actors) do
  collide(player,a)
 end
 if(level==2)then
	collide(car,hoop)
 end
end

function collide_event(a1,a2)
 if a2.kind == 1 then
  a2.i = 100
  rings -= 1
  player.score += 1
  del(actors,a2)
  score = score + 1
  sfx(52)
  sfx(53)
 elseif a2.kind == 2 or a2.kind == 4 then
    player.life = 0
 elseif a2.kind == 5 then
	score += 10
	next = true
 end
end
 
function update_ring(r)
	r.i += 1
	if r.i == 41 then
		r.i = 0 
	end
	if r.i <=10 then 
		r.frame = 24
	elseif r.i >10 and r.i <= 20 then r.frame = 40
	elseif r.i > 20 and r.i <= 30 then r.frame = 56
	elseif r.i > 30 and r.i <= 40 then r.frame = 25
	elseif r.i >= 100 and r.i < 110 then r.frame = 41
	elseif r.i >= 110 and r.i < 125 then r.frame = 57
	elseif r.i == 125 then  end
	
	r.dx = -1.5
	if(r.x < -8) then r.x = 136 end
end

function move_obstacle(o)
	o.dx = -0.5
    if(o.x < -8) then 
        o.x = 136
        o.y = player.y
    end
end

function move_car(c)
    if(c.y < 100) then
        c.dy += c.ddy
    else
        c.dy = 0
    end
	if(once)then
		c.dy -= c.power
		once = false
	end
	if(active)then 
		c.dx = 1
		c.ddy = 0.06
	end
	if(c.power < 5 and power_dir == 1) then
		c.power += .03
	else
		power_dir = -1
	end
	if(c.power > .0001 and power_dir == -1) then
		c.power -= .03
	else
		power_dir = 1
	end
end

function move_enemy(e)
	e.dx = -1
    e.dy = 1
    if(e.x < -8) then e.x = 136 end
    if(e.y > 120) then e.y = -30 - rnd(30) end
end

function move_actor(actor)
	if(actor.kind==0)then
		move_player(actor)
		if actor.x <= 0 then actor.x = 1 actor.dx = 0 end
		if actor.x >= 110 then actor.x = 108 actor.dx = 0 end
		if actor.y <= 0 then actor.y = 1 actor.dy = 0 end
		if actor.y >= 120 then actor.y = 118 actor.dy = 0 end
	end
	
	if(actor.kind==1)then
		update_ring(actor)
	end
	
	if(actor.kind==2)then
		move_obstacle(actor)
	end
	
	if(actor.kind==3)then
		move_car(actor)
	end
	
	if(actor.kind==4)then
		move_enemy(actor)
	end
	
	actor.x += actor.dx
	actor.y += actor.dy
end

function move_map()
	if level==1 then
		for b in all(currentlvl) do
			if(b.x-x_dist < -8)then b.x += 128*2+136 end
		end
	end
end

function check_win_lose()
	if(player.score == 5 and level==1)then next = true end
	if next then
		level += 1
		if(level >2)then level=1 end
		next = false
		if (level==1) then 
		active = false
			run_level1()
		elseif(level==2) then 
			run_level2()
			player.score = 0
		end
	end
	if(timer < 1 and level == 1) then
		player.life = 0
	end
	if(timer < 1 and level == 2) then
	 next = true
	end
end

function _update60()
	if(player.life == 0) then
		if btn(4) then 
			player.life = 1 _init() 
		end
	end
	--move actors
	foreach(actors, move_actor)
	--move effects
	move_map()
	timer -= .01
	collisions()
	--check for win/lose
	check_win_lose()
	if(x_dist%108 == 0 and rings < max_rings and level==1) then
		make_ring(136,prev_r.y + 20 - rnd(40),1)
	end
    if(x_dist%108 == 0 and enemies < max_enemies and level==1) then
        	make_enemy(140,-60 + rnd(60),1)
	end
end

function draw_actor(a)
	if(a.d == 1) then
	 spr(a.frame,a.x,a.y,a.w,a.h)
	elseif (a.d == 0) then
	 spr(a.frame,a.x,a.y,a.w,a.h,1)
	elseif (a.d == 2) then
	 spr(16,a.x,a.y,1,2)
	elseif (a.d == 3) then
	 spr(16,a.x,a.y,1,2,1,1)
	end
end

function draw_map_block(b)
	if(level==1)then
		spr(b.kind,
			b.x-x_dist,
			b.y,
			b.w,
			b.h)
	else
		spr(b.kind,
			b.x,
			b.y,
			b.w,
			b.h)
	end
end

function _draw()
 if player.life == 0 then
	cls()
	print("hit z to try again!",50,50)
	print("final score: ".. score,50,70)
	spr(3,7,95,3,5)
	spr(50,0,120,6,1)
 else 
	cls()
	--set camera
	--draw background
	--draw actors and map
	foreach(currentlvl,draw_map_block)
	foreach(actors,draw_actor)
	print("“:" .. flr(timer),10,10,8)
print("score:" .. score,90,10,8)
	if(level == 2) then
		print("power: " .. car.power,7,105,8)
	end
	--draw hud
	--draw end result
 end
end

